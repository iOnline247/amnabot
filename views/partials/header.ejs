<header>
	<nav class="navbar navbar-default">
        <div class="notification-bar pull-right">
        </div>
		<div class="container-fluid">
			<div class="navbar-header">
				<ul class="nav navbar-nav">
				<% if (userName === 'Home') { %>
				<li>
					<a class="navbar-brand" href="/">Home</a>
				</li>
				<% } else { %>
				<li>
					<a href="/">
						<img src="https://twitter.com/<%= userName %>/profile_image" alt="<%= userName %>"/><%= userName %>!
					</a>
				</li>
				<% } %>
				<% if (loggedIn) { %>
				<li class="navbar-brand"><a href="/logout">Logout</a></li>
				<% } else { %>
				<li><a href="/login">Login</a></li>
				<% } %>
				</ul>
			</div>
		</div><!-- /.container-fluid -->
	</nav>
</header>